{
  "name": "vscode-extention",
  "version": "1.0.0",
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-coding-time-tracker.showStats",
        "title": "コーディング統計を表示"
      },
      {
        "command": "vscode-coding-time-tracker.showUserId",
        "title": "ユーザーIDを表示"
      },
      {
        "command": "vscode-coding-time-tracker.previewTsxFile",
        "title": "TSXファイルをプレビュー"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceExtname == .tsx",
          "command": "vscode-coding-time-tracker.previewTsxFile",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "VSCode用コーディング時間追跡拡張機能",
  "dependencies": {
    "firebase": "^11.6.0",
    "vite-plugin-vue-component-preview": "^1.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "esbuild": "^0.19.0"
  }
}
